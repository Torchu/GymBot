{
  "name": "GymBot",
  "version": "1.0.0",
  "description": "Bot de Telegram que almacena tu rutina de ejercicios del gimnasio y te recuerda cada día qué ejercicios tienes que hacer.",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "mongoose": "^5.11.12",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.19",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.13",
    "@types/supertest": "^2.0.10",
    "grunt": "^1.3.0",
    "grunt-run": "^0.8.1",
    "jest": "^26.6.3",
    "nodemon": "^2.0.6",
    "supertest": "^6.0.1",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "scripts": {
    "start": "node dist/server.js",
    "dev": "nodemon src/server.ts",
    "build": "tsc",
    "test": "jest",
    "coverage": "jest --coverage --coverageDirectory='docs/coverage' --no-cache && xdg-open docs/coverage/lcov-report/index.html",
    "today": "node dist/assets/example_routine.js > functions/today.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Torchu/GymBot.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Torchu/GymBot/issues"
  },
  "homepage": "https://github.com/Torchu/GymBot#readme"
}
